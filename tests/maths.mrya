let math = import("package:math")

output math.abs(-5) // 5
output math.abs(0)  // 0
output math.abs(3.5) // 3.5
output math.abs(-3.5) // 3.5

output math.round(3.2) // 3
output math.round(3.7) // 4
output math.round(-3.7) // -4

output math.ceil(3.2) // 4
output math.ceil(-3.7) // -3

output math.floor(3.7) // 3
output math.floor(-3.2) // -4

output math.sqrt(16) // 4
output math.sqrt(2)  // ~1.414

output math.random() // Should be between 0 and 1

output math.randint(1, 10) // Should be between 1 and 10 inclusive
output math.randint(-10, -1) // Should be between -10 and -1 inclusive
output math.randint(-5, 5) // Should be between -5 and 5 inclusive

// New math functions tests
output math.sin(0) // 0
output math.cos(0) // 1
output math.tan(0) // 0

output math.log(1) // 0
output math.exp(0) // 1

output math.pow(2, 3) // 8
output math.pow(10, 2) // 100
